#include <stdlib.h>
#include <time.h>
#include <stdio.h>

/**
* main - starting point for program
* Return: 0 is no error, non zero if error
*/

int main(void)
{
	int n;

	srand(time(0));
	n = rand() - RAND_MAX / 2;

	int last = n % 10;

	if (isdigit > 5)
		printf("Last digit of %d is %d and  is grater than 5\n", last);
	return (0);
}
